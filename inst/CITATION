citHeader("To cite pear in publications use:")

citEntry(entry = "Article",
  title        = "periodicAR: Periodic Autoregression Analysis",
  author       = personList(as.person("Mehmet Balcilar")),
  journal      = "R Package",
  year         = "2023",
  url          = "https://github.com/mbalcilar/periodicAR",

  textVersion  =
  paste("Balcilar, Mehmet (2023).",
        "periodicAR: Periodic Autoregression Analysis",
        "Available online: https://github.com/mbalcilar/periodicAR")
)



